number = {30},
pages = {12172--12179},
pmid = {23812749},
title = {{Maturation, not initiation, is the major roadblock during reprogramming toward pluripotency from human fibroblasts}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1310291110},
volume = {110},
