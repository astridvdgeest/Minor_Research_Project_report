keywords = {Cell Differentiation,Cell Line,Cell Separation,Cell Separation: methods,Embryo, Mammalian,Embryo, Mammalian: cytology,Humans,Neurons,Neurons: cytology,Neurons: metabolism,Stem Cells,Stem Cells: classification,Stem Cells: cytology,Stem Cells: metabolism,Synapses,Synapses: metabolism,Transcription Factors,Transcription Factors: genetics,Transcription Factors: metabolism},
