issn = {22136711},
journal = {Stem Cell Reports},
keywords = {BMP,TDi,ectoderm,embryonic stem cells,endogenous signal control,fully defined differentiation,human pluripotent stem cells,neural crest,top-down inhibition},
number = {4},
pages = {1043--1052},
pmid = {28919261},
publisher = {ElsevierCompany.},
title = {{Top-Down Inhibition of BMP Signaling Enables Robust Induction of hPSCs Into Neural Crest in Fully Defined, Xeno-free Conditions}},
volume = {9},
year = {2017}
}
